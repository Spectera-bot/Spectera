datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GuildSettings {
  id  String  @unique
  prefixes  String[]
  autorole  String?
  muterole  String?
  modRoles  String[]
  tags  String[]
}

model GuildLogs {
  id String @unique
  messageLogChannel String
  modLogChannel String
  memberLogChannel String
  phishLogChannel String
}
