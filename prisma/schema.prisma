datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GuildSettings {
  id  String  @unique
  prefixes  String[]
  autorole  String?
  loggingChannel  String?
  muterole  String?
  modRoles  String[]
  tags  String[]
}