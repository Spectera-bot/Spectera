{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"allowJs": true,
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"composite": true,
		"paths": {
			"#structures/*": ["lib/structures/*.js"],
			"#types/enums/*": ["lib/types/enums/*.js"],
			"#types/interfaces/*": ["lib/types/interfaces/*.js"],
			"#schemas/*": ["database/schemas/*.js"],
			"#database/*": ["database/*.js"],
			"#SchemaTypes/*": ["lib/types/SchemaTypes/*.js"],
		}
	},
	"include": [".", "./**/*.json"],
	"exclude": ["./tsconfig.json"]
}
