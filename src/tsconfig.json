{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"allowJs": true,
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"paths": {
			"#structures/*": ["lib/structures/*.js"],
			"#types/enums/*": ["lib/types/enums/*.js"],
			"#types/interfaces/*": ["lib/types/interfaces/*.js"],
			"#lib/*": ["lib/*.js"],
			"#root/*": ["*"]
		},
		"composite": true,
		"noUnusedLocals": false
	},
	"include": [".", "./**/*.json"],
	"exclude": ["./tsconfig.json"]
}
