{
	"name": "obligatorsapphire",
	"version": "3.0.0",
	"description": "",
	"main": "dist/Obligator.js",
	"keywords": [],
	"author": "UndiedGamer",
	"license": "Apache-2.0",
	"dependencies": {
		"@sapphire/decorators": "next",
		"@sapphire/discord.js-utilities": "next",
		"@sapphire/framework": "next",
		"@sapphire/plugin-subcommands": "next",
		"@sapphire/utilities": "next",
		"discord.js": "^13.1.0",
		"dotenv-cra": "^3.0.1",
		"mongoose": "^6.0.2"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"scripts": {
		"build:esbuild": "node scripts/build.mjs",
		"build": "yarn clean && yarn build:esbuild && yarn typecheck",
		"watch": "node scripts/watch.mjs",
		"clean": "node scripts/clean.mjs",
		"start": "yarn lint && yarn build:esbuild && node ./dist/Obligator.js",
		"check:lint": "prettier -c \"src/**/*.ts\"",
		"typecheck": "tsc -p src --noEmit",
		"dev": "run-s build start",
		"lint": "prettier --write \"src/**/*.ts\"",
		"predocker:watch": "npm install",
		"docker:watch": "tsc-watch --onSuccess \"node ./dist/index.js\"",
		"update": "yarn add @sapphire/framework@next @sapphire/utilities@next @sapphire/discord.js-utilities@next @sapphire/plugin-subcommands@next @sapphire/decorators@next"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-decorators": "^7.14.5",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-typescript": "^7.15.0",
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-angular": "^13.1.0",
		"@sapphire/eslint-config": "^3.2.3",
		"@sapphire/plugin-api": "next",
		"@sapphire/plugin-editable-commands": "next",
		"@sapphire/plugin-logger": "^1.0.13",
		"@sapphire/ts-config": "^3.0.0",
		"@types/node": "^16.7.6",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"babel-plugin-const-enum": "^1.1.0",
		"colorette": "^1.3.0",
		"esbuild": "^0.12.24",
		"eslint": "^7.32.0",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^10.2.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.2"
	},
	"imports": {
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/enums/*": "./dist/lib/types/enums/*.js",
		"#types/interfaces/*": "./dist/lib/types/interfaces/*.js",
		"#schemas/*": "./dist/database/schemas/*.js",
		"#database/*": "./dist/database/*.js",
		"#SchemaTypes/*": "./dist/lib/types/SchemaTypes/*.js"
	},
	"packageManager": "yarn@3.0.1"
}
