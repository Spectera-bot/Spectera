{
	"name": "obligatorsapphire",
	"version": "3.0.0",
	"description": "",
	"main": "dist/index.js",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@sapphire/decorators": "^3.0.0-next.6e8314f3.0",
		"@sapphire/discord.js-utilities": "^2.0.2",
		"@sapphire/framework": "^2.0.0-next.d4386ff5.0",
		"@sapphire/utilities": "^2.1.0-next.6e8314f3.0",
		"discord.js": "^13.1.0"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"scripts": {
		"ts-start": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"start": "yarn lint && tsc && node .",
		"lint": "prettier --write .",
		"build": "tsc --diagnostics",
		"build:js": "tsc",
		"check": "yarn prettier -c . && yarn eslint . --ext .ts"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-angular": "^13.1.0",
		"@types/node": "^16.6.1",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"eslint": "^7.32.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"ts-node": "^10.2.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.3.5"
	},
	"engines": {
		"yarn": ">=3.0.1"
	}
}
