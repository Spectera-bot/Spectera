{
	"name": "spectera",
	"version": "0.5.0",
	"description": "",
	"main": "dist/Spectera.js",
	"keywords": [],
	"author": "UndiedGamer",
	"license": "Apache-2.0",
	"scripts": {
		"build:esbuild": "node scripts/build.mjs",
		"postinstall": "husky install .github/husky",
		"build": "yarn clean && yarn build:esbuild",
		"watch": "node scripts/watch.mjs",
		"clean": "node scripts/clean.mjs",
		"start": "yarn lint && yarn build && node --enable-source-maps .",
		"docker:start": "node --enable-source-maps .",
		"check:lint": "prettier -c \"src/**/*.ts\"",
		"typecheck": "tsc --noEmit",
		"dev": "run-s build start",
		"lint": "prettier --write \"src/**/*.ts\"",
		"predocker:watch": "yarn",
		"docker:watch": "tsc-watch -b src --onSuccess \"node . --enable-source-maps\""
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"dependencies": {
		"@prisma/client": "3.3.0",
		"@sapphire/decorators": "^3.1.3",
		"@sapphire/discord.js-utilities": "^4.0.1",
		"@sapphire/eslint-config": "4.0.2",
		"@sapphire/framework": "^2.1.3",
		"@sapphire/plugin-api": "^3.0.1",
		"@sapphire/plugin-editable-commands": "^1.0.1",
		"@sapphire/plugin-logger": "^2.0.1",
		"@sapphire/plugin-subcommands": "^2.0.1",
		"@sapphire/prettier-config": "^1.2.3",
		"@sapphire/stopwatch": "1.2.3",
		"@sapphire/time-utilities": "^1.4.6",
		"@sapphire/ts-config": "3.1.3",
		"@sapphire/type": "^2.1.1",
		"@sapphire/utilities": "^3.0.6",
		"discord.js": "^13.2.0",
		"dotenv-cra": "^3.0.2",
		"vm2": "^3.9.5"
	},
	"devDependencies": {
		"@commitlint/cli": "13.2.1",
		"@commitlint/config-angular": "13.2.0",
		"@commitlint/config-conventional": "13.2.0",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "5.2.0",
		"@typescript-eslint/parser": "5.2.0",
		"colorette": "2.0.16",
		"esbuild": "^0.13.10",
		"eslint": "8.1.0",
		"husky": "7.0.4",
		"lint-staged": "11.2.6",
		"prettier": "2.4.1",
		"pretty-quick": "^3.1.1",
		"prisma": "3.3.0",
		"reflect-metadata": "0.1.13",
		"tsc-watch": "4.5.0",
		"typescript": "^4.4.4"
	},
	"imports": {
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/enums/*": "./dist/lib/types/enums/*.js",
		"#types/interfaces/*": "./dist/lib/types/interfaces/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js"
	},
	"packageManager": "yarn@3.0.2",
	"prettier": "@sapphire/prettier-config"
}
