{
	"name": "obligatorsapphire",
	"version": "3.0.0",
	"description": "",
	"main": "dist/Obligator.js",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@sapphire/decorators": "next",
		"@sapphire/discord.js-utilities": "next",
		"@sapphire/framework": "next",
		"@sapphire/plugin-subcommands": "next",
		"@sapphire/utilities": "next",
		"discord.js": "^13.1.0",
		"mongoose": "^6.0.2"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"scripts": {
		"start-tsn": "yarn lint && yarn ts-start",
		"ts-start": "ts-node-dev --respawn --transpile-only --poll ./src/Obligator.ts",
		"start": "yarn lint && tsc -b src && node .",
		"lint": "prettier --write . && yarn eslint . --ext .ts --fix",
		"build": "tsc --diagnostics",
		"build:js": "tsc -b src",
		"check:lint": "yarn prettier -c . && yarn eslint . --ext .ts"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-angular": "^13.1.0",
		"@sapphire/eslint-config": "^3.2.3",
		"@sapphire/plugin-api": "next",
		"@sapphire/plugin-editable-commands": "next",
		"@sapphire/plugin-logger": "^1.0.13",
		"@sapphire/ts-config": "^3.0.0",
		"@types/node": "^16.7.2",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"colorette": "^1.3.0",
		"eslint": "^7.32.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^10.2.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.2"
	},
	"imports": {
		"#config/*": "./dist/config/*.json",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/enums/*": "./dist/lib/types/enums/*.js",
		"#types/interfaces/*": "./dist/lib/types/interfaces/*.js",
		"#schemas/*": "./dist/database/schemas/*.js",
		"#database/*": "./dist/database/*.js",
		"#SchemaTypes/*": "./dist/lib/types/SchemaTypes/*.js"
	}
}
