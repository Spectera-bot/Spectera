{
	"name": "obligatorsapphire",
	"version": "3.0.0",
	"description": "",
	"main": "dist/index.js",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@sapphire/decorators": "^3.0.0-next.6e8314f3.0",
		"@sapphire/discord.js-utilities": "^3.0.0-next.2c2df24f.0",
		"@sapphire/eslint-config": "^3.2.3",
		"@sapphire/framework": "^2.0.0-next.a48b0764.0",
		"@sapphire/plugin-subcommands": "^2.0.0-next.7a9739f.0",
		"@sapphire/snowflake": "^2.0.0",
		"@sapphire/ts-config": "^3.0.0",
		"@sapphire/utilities": "^2.1.0-next.6e8314f3.0",
		"discord.js": "^13.1.0",
		"dotenv-cra": "^3.0.1",
		"mongoose": "^5.13.7"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"scripts": {
		"ts-start": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"start": "yarn lint && tsc -b src && node .",
		"lint": "prettier --write .",
		"build": "tsc --diagnostics",
		"build:js": "tsc",
		"check:lint": "yarn prettier -c . && yarn eslint . --ext .ts"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-angular": "^13.1.0",
		"@types/node": "^16.7.1",
		"@typescript-eslint/eslint-plugin": "^4.29.2",
		"@typescript-eslint/parser": "^4.29.2",
		"eslint": "^7.32.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"ts-node": "^10.2.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.3.5"
	},
	"imports": {
		"#config/*": "./dist/config/*.json",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/enums/*": "./dist/lib/types/enums/*.js",
		"#types/interfaces/*": "./dist/lib/types/interfaces/*.js",
		"#schemas/*": "./dist/database/schemas/*.js",
		"#database/*": "./dist/database/*.js",
		"#Schema/*": "./dist/lib/types/SchemaTypes/*.js"
	}
}
