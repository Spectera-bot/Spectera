{
	"name": "spectera",
	"version": "3.0.0",
	"description": "",
	"main": "dist/Spectera.js",
	"keywords": [],
	"author": "UndiedGamer",
	"license": "Apache-2.0",
	"dependencies": {
		"@prisma/client": "2.30.3",
		"@sapphire/decorators": "next",
		"@sapphire/discord.js-utilities": "next",
		"@sapphire/framework": "next",
		"@sapphire/plugin-subcommands": "next",
		"@sapphire/prettier-config": "^1.1.6",
		"@sapphire/utilities": "next",
		"discord.js": "^13.1.0",
		"dotenv-cra": "^3.0.2"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"scripts": {
		"build:esbuild": "node scripts/build.mjs",
		"build": "yarn clean && yarn build:esbuild",
		"watch": "node scripts/watch.mjs",
		"clean": "node scripts/clean.mjs",
		"start": "yarn lint && yarn build && node .",
		"check:lint": "prettier -c \"src/**/*.ts\"",
		"typecheck": "tsc -p src --noEmit",
		"dev": "run-s build start",
		"lint": "prettier --write \"src/**/*.ts\"",
		"predocker:watch": "npm install",
		"docker:watch": "tsc-watch --onSuccess \"node ./dist/index.js\"",
		"update": "yarn add @sapphire/framework@next @sapphire/utilities@next @sapphire/discord.js-utilities@next @sapphire/plugin-subcommands@next @sapphire/decorators@next @sapphire/plugin-editable-commands@next"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^3.2.3",
		"@sapphire/plugin-api": "next",
		"@sapphire/plugin-editable-commands": "next",
		"@sapphire/plugin-logger": "^1.0.13",
		"@sapphire/stopwatch": "^1.1.4",
		"@sapphire/ts-config": "^3.0.0",
		"@sapphire/type": "^2.1.0",
		"@types/node": "^16.9.1",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"@yarnpkg/sdks": "^2.4.2-rc.3",
		"colorette": "^1.4.0",
		"esbuild": "^0.12.26",
		"eslint": "^7.32.0",
		"lint-staged": "^11.1.2",
		"prettier": "^2.4.0",
		"prisma": "2.30.3",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^10.2.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.3"
	},
	"imports": {
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/enums/*": "./dist/lib/types/enums/*.js",
		"#types/interfaces/*": "./dist/lib/types/interfaces/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js"
	},
	"packageManager": "yarn@3.0.1",
	"prettier": "@sapphire/prettier-config"
}
